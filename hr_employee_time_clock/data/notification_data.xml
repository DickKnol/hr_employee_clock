<?xml version="1.0" encoding='UTF-8'?>
<odoo>
	<record id="hr_employee_time_clock_notification_cron" model="ir.cron">
        <field name="name">Attendance sheet last day notification</field>
        <field name="model_id" ref="model_hr_timesheet_sheet_sheet"/>
        <field name="state">code</field>
        <field name="code">model.recompute_last_day_notification()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>
</odoo>